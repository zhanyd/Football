<!--     按条件查询 -->
	<sql id="searchTeamManageSql">
		select m.ID,m.TeamName,m.CaptainID,t.RealName as CaptainName,m.Wins,m.Draw,m.Losing,m.Goals,m.Lost,m.Balance,m.CreateTime,m.UpdateTime from team m left join tuser t on  m.CaptainID = t.ID
		<dynamic prepend="WHERE">
			<isNotEmpty prepend="and" property="teamName">
				m.TeamName like '%$teamName$%'
			</isNotEmpty>
		</dynamic>
		order by m.CreateTime desc
	</sql>
	<select id="searchTeamManage" parameterClass="com.wildwolf.model.Team" resultMap="BaseResultMap">
		<include refid="searchTeamManageSql" />
	</select>


